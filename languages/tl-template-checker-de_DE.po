#: includes/admin/views/html-admin-page-status-report.php:49
#, php-format
msgid ""
"%s <code>%s</code>: Child theme version <strong style=\"color:red\">%s</"
"strong> is out of date. The parent theme version is <strong>%s</strong>."
msgstr ""
"%s <code>%s</code>: Child-Theme-Version <strong style=\"color:red\">%s</"
"strong> ist veraltet. Die Version aus dem Eltern-Theme lautet <strong>%s</"
"strong>."

#: includes/admin/views/html-admin-page-status-report.php:57
#, php-format
msgid ""
"%s <code>%s</code>: Child theme is missing version keyword. The parent theme "
"version is <strong>%s</strong>."
msgstr ""
"%s <code>%s</code>: Child-Theme hat keine @version-Information im Header. "
"Die Version aus dem Eltern-Theme lautet <strong>%s</strong>."

#: includes/admin/views/html-admin-page-status-report.php:64
#, php-format
msgid "%s <code>%s</code>: Parent theme is missing version keyword."
msgstr "%s <code>%s</code>: Versionsinfo fehlt im Header des Eltern-Themes."

#: includes/admin/views/html-admin-page-status-report.php:70
#, php-format
msgid ""
"%s <code>%s</code>: Child theme version <strong style=\"color:green\">%s</"
"strong> matches parent theme."
msgstr ""
"%s <code>%s</code>: Child-Theme-Version <strong style=\"color:green\">%s</"
"strong> ist mit Version aus dem Eltern-Theme identisch."

#: includes/admin/views/html-admin-page-status-report.php:87
#, php-format
msgid ""
"Template overrides by <abbr title=\"Child theme\">%s</abbr> for <abbr title="
"\"Parent theme\">%s</abbr>:"
